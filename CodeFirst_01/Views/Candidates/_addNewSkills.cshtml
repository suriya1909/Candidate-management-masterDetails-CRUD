@model Skill

<div class="row d-flex justify-content-between mt-2">
    <div class="offset-3 col-7">
        @Html.DropDownListFor(x => x.SkillId, ViewBag.skill as SelectList,"---select---",new {@class = "form-select form-select-sm"})
    </div>
    <div class="col-2">
        <a id="btnDelete" class="btn btn-danger"><i class="fa fa-trash"></i></a>
    </div>
</div>
